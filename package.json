    "type": "git",
    "url": "git+https://github.com/freelancernasimofficial/nascoder-terminal-browser-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/freelancernasimofficial/nascoder-terminal-browser-mcp/issues"
  },
  "homepage": "https://github.com/freelancernasimofficial/nascoder-terminal-browser-mcp#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
