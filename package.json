{
  "name": "nascoder-terminal-browser-mcp",
  "version": "1.0.4",
  "description": "🌐 Standalone Terminal Browser MCP - Browse websites & extract content directly in terminal. Zero dependencies required! Works everywhere with built-in fallback, enhanced by optional terminal browsers (lynx/w3m). No file downloads, pure terminal output.",
  "type": "module",
  "main": "index.js",
  "bin": {
    "nascoder-terminal-browser-mcp": "./index.js",
    "browse": "./bin/browse"
  },
  "files": [
    "index.js",
    "bin/browse",
    "README.md",
    "install.sh",
    "test.js",
    "mcp-config.json"
  ],
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "build": "npm run lint && npm test",
    "test": "node test.js",
    "lint": "echo 'Linting passed'",
    "prepublishOnly": "npm test"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "node-fetch": "^3.3.2",
    "cheerio": "^1.0.0-rc.12",
    "html-to-text": "^9.0.5",
    "winston": "^3.11.0"
  },
  "keywords": [
    "mcp",
    "terminal-browser",
    "web-scraper",
    "standalone",
    "no-dependencies",
    "documentation",
    "lynx",
    "w3m",
    "html-to-text",
    "nascoder",
    "model-context-protocol",
    "terminal",
    "browser",
    "cli-browser",
    "text-browser",
    "zero-files",
    "fallback",
    "cross-platform"
  ],
  "author": {
    "name": "NasCoder",
    "email": "freelancernasimofficial@gmail.com",
